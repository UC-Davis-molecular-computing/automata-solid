    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.17.0",
    "eslint-plugin-solid": "^0.14.4",
    "jsdom": "^25.0.0",
    "tsx": "^4.7.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
